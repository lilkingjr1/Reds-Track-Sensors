<?xml version="1.0" encoding="UTF-8"?>
<root>
  <material>
    <description>Matte, black colored, plastic</description>
    <renderpass>
      <shadercore>Roughness = 0.4;
Metallic = 0.03;</shadercore>
      <transparency mode="opaque"/>
      <linearshading/>
      <custommaterial>
        <diffuse r="0.117647" g="0.117647" b="0.117647"/>
        <ambient r="0.117647" g="0.117647" b="0.117647"/>
      </custommaterial>
      <rgbgen mode="lighting"/>
    </renderpass>
  </material>
</root>